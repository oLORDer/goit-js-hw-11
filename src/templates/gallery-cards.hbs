{{#each this}}
  <a href='{{this.largeImageURL}}'><div class='photo-card'>
      <img
        src='{{this.webformatURL}}'
        alt='{{this.tags}}'
        loading='lazy'
        width='400'
        height='300'
      />
      <div class='info'>
        <p class='info-item'>
          <b>Likes: {{this.likes}}</b>
        </p>
        <p class='info-item'>
          <b>Views: {{this.views}}</b>
        </p>
        <p class='info-item'>
          <b>Comments: {{this.comments}}</b>
        </p>
        <p class='info-item'>
          <b>Downloads: {{this.downloads}}</b>
        </p>
      </div>
    </div>
  </a>
{{/each}}